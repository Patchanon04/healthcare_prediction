{
  "dashboard": {
    "title": "ML Service - Brain Tumor Detection",
    "tags": ["ml", "fastapi", "predictions"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Predictions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ml_predictions_total)",
            "legendFormat": "Total Predictions"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Models Loaded",
        "type": "stat",
        "targets": [
          {
            "expr": "ml_models_loaded",
            "legendFormat": "Models"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Prediction Rate (per minute)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ml_predictions_total[5m]) * 60",
            "legendFormat": "Predictions/min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 4,
        "title": "Prediction Latency (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ml_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 latency"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ml_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 latency"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 5,
        "title": "Predictions by Result",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (result) (ml_predictions_total)",
            "legendFormat": "{{result}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12}
      },
      {
        "id": 6,
        "title": "Predictions by Model",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (model) (ml_predictions_total)",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12}
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ml_prediction_errors_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12}
      },
      {
        "id": 8,
        "title": "Model Confidence Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(ml_model_confidence_bucket[5m])",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20}
      }
    ],
    "refresh": "10s",
    "time": {"from": "now-1h", "to": "now"}
  }
}
